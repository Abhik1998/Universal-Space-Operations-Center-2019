<TabPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller = "com.ksatstuttgart.usoc.gui.controller.LogPanelController" maxWidth="270.0" minWidth="270.0" prefWidth="270.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER"> 
  <tabs>
    <Tab text="Serial"> 
      <content> 

        <BorderPane>
        <padding><Insets top="5" right="5" bottom="5" left="5"/></padding>
          <top>
            <GridPane alignment="TOP_CENTER" BorderPane.alignment="CENTER">
            <padding><Insets bottom="5" /></padding>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
              <children>
                <Button text="Connect" onAction="#serialConnect" GridPane.columnIndex="1" GridPane.rowIndex="1" maxWidth="-Infinity" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES" GridPane.halignment="RIGHT" GridPane.hgrow="SOMETIMES" mnemonicParsing="false" />
                <Button text="Send Command" onAction="#serialSendCommand" GridPane.columnIndex="1" GridPane.rowIndex="2" maxWidth="-Infinity" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES" GridPane.halignment="RIGHT" GridPane.hgrow="SOMETIMES" mnemonicParsing="false" />
                <ComboBox fx:id="comboBox1" promptText="Baudrates" GridPane.columnIndex="0" GridPane.rowIndex="0" maxWidth="135.0" minWidth="135.0" prefWidth="135.0" GridPane.halignment="LEFT" GridPane.hgrow="SOMETIMES" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES" >
                  <items>
                    <FXCollections fx:factory="observableArrayList">
                    </FXCollections>
                  </items>
                </ComboBox>
                <ComboBox fx:id="comboBox2" promptText="Ports" GridPane.columnIndex="0" GridPane.rowIndex="1" maxWidth="135.0" minWidth="135.0" prefWidth="135.0" GridPane.halignment="LEFT" GridPane.hgrow="SOMETIMES" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES">
                  <items>
                    <FXCollections fx:factory="observableArrayList">
                    </FXCollections>
                  </items>
                </ComboBox>
                <ComboBox fx:id="comboBox3" promptText="Commands" GridPane.columnIndex="0" GridPane.rowIndex="2" maxWidth="135.0" minWidth="135.0" prefWidth="135.0" GridPane.halignment="LEFT" GridPane.hgrow="SOMETIMES" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES">
                  <items>
                    <FXCollections fx:factory="observableArrayList">           
                    </FXCollections>
                  </items>
                </ComboBox>
              </children>
            </GridPane>
          </top>
          <center>
            <TextArea fx:id="serialTextArea" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
          </center>
        </BorderPane>

      </content> 
    </Tab>
    <Tab text="Iridium"> 
      <content> 

        <BorderPane>
        <padding><Insets top="5" right="5" bottom="5" left="5"/></padding>
          <top>
            <GridPane alignment="TOP_CENTER" BorderPane.alignment="CENTER">
            <padding><Insets bottom="5" /></padding>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" prefWidth="90.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
